package data;
/**
 * socket消息类 
 * socket传输时用此类包装message
 * @author yunlong3
 *
 */
public class BOSocketMessage {	
	private String name;
	private int action;
	
	public void setName(String name){
		this.name = name;
	}
	
	public void setAction(int action){
		this.action = action;
	}
	
	public String getName(){
		return this.name;
	}
	
	public int getAction(){
		return this.action;
	}
}
